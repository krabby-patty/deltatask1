<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/correctColor">

    <TextView
        android:id="@+id/FINDFAC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find the factor of     "
        android:fontFamily="@font/azo"
        android:layout_marginTop="200dp"
        android:textSize="20sp"
        android:textColor="@color/yellow"
        android:layout_centerHorizontal="true"
        />

    <TextView
        android:id="@+id/prnt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/FINDFAC"
        android:fontFamily="@font/azo"
        android:layout_marginTop="200dp"
        android:text="0"
        android:textColor="@color/yellow"
        android:textSize="20sp"

        />

    <Button
        android:id="@+id/btn32"
        android:layout_width="200dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button"
        android:fontFamily="@font/azo"
        android:textColor="@color/blue"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_marginTop="250dp"
        />

    <Button
        android:id="@+id/btn42"
        android:layout_width="200dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button"
        android:fontFamily="@font/azo"
        android:textColor="@color/blue"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_marginTop="310dp"
        />

    <Button
        android:id="@+id/btn52"
        android:layout_width="200dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button"
        android:fontFamily="@font/azo"
        android:textColor="@color/blue"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_marginTop="370dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/yellow"
        android:fontFamily="@font/azo"

        android:text="CORRECT ANSWER!"
        android:layout_marginTop="425dp"
        android:layout_centerHorizontal="true"
        android:textSize="25sp"

         />

    <Button
        android:id="@+id/btn6"
        android:layout_width="200dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button"
        android:fontFamily="@font/azo"
        android:textColor="@color/blue"
        android:layout_height="wrap_content"
        android:text="Next question"

        android:layout_marginTop="490dp"
        />

    <TextView
        android:id="@+id/streakprint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="455dp"
        android:textColor="@color/yellow"
        android:fontFamily="@font/azo"
        android:textSize="25sp"
        />

    <Button
        android:id="@+id/btn7"
        android:layout_width="200dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button"
        android:fontFamily="@font/azo"
        android:textColor="@color/blue"
        android:layout_height="wrap_content"

        android:layout_marginTop="555dp"
        android:text="Main menu"
        />
<TextView
    android:id="@+id/hiskore"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:fontFamily="@font/azo"

    android:text="NEW HIGHSCORE!"
    android:textColor="@color/yellow"
    android:textSize="30sp"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="150dp"
    />
</RelativeLayout>